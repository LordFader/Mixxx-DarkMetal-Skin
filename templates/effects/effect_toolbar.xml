<!DOCTYPE template>

<Template>

		<!-- Texts -->
		<WidgetGroup>
		<ObjectName>Texts</ObjectName>	
		<MinimumSize>55,30</MinimumSize>
		<SizePolicy>f,f</SizePolicy>
		<Layout>vertical</Layout>
		<Children>
		<Label>
		<Style>font:10px/12px; background:transparent; color:#484848;</Style>
		<Text>Effects</Text>
		</Label>
		</Children>			
		</WidgetGroup>
		
		<!-- Button: Minimize/Maximize this Row -->
		<PushButton>	
		<NumberStates>2</NumberStates>
		<State>
		<Number>0</Number>
		<Pressed>skin:graphics/btn_row_maximize.svg</Pressed>
		<Unpressed>skin:graphics/btn_row_maximize.svg</Unpressed>
		</State>
		<State>
		<Number>1</Number>
		<Pressed>skin:graphics/btn_row_minimize.svg</Pressed>
		<Unpressed>skin:graphics/btn_row_minimize.svg</Unpressed>
		</State>		
		<Connection>
		<ConfigKey>[EffectsMaximize],show</ConfigKey>
		</Connection>
		</PushButton>
		
		<!-- Button: Show/Hide this Row -->
		<PushButton>	
		<NumberStates>2</NumberStates>
		<State>
		<Number>0</Number>
		<Pressed>skin:graphics/btn_view1.svg</Pressed>
		<Unpressed>skin:graphics/btn_view1.svg</Unpressed>
		</State>
		<State>
		<Number>1</Number>
		<Pressed>skin:graphics/btn_view1_pressed.svg</Pressed>
		<Unpressed>skin:graphics/btn_view1_pressed.svg</Unpressed>
		</State>		
		<Connection>
		<ConfigKey>[EffectsRack],show</ConfigKey>
		</Connection>
		</PushButton>
		
